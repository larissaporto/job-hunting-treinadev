<%= form_with model: profile, local: true do |f| %>

    <p>
    <%= f.label :name  %>
    <%= f.text_field :name %>
    </p>
    <p>
    <%= f.label :social_name %>
    <%= f.text_field :social_name %>
    </p>
    <p>
    <%= f.label :birth_date %>
    <%= f.date_field :birth_date %>
    </p>
    <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
    </p>
    <p>
    <%= f.label :experience %>
    <%= f.text_area :experience %>
    </p>

    <div>
    <%= f.label :qualification %>
    <%= f.select(:qualification, Profile.qualifications.keys, {}, class: 'profile-qualifications-select' ) %>
    </div>

    <div>
    <%= f.label :photo %>
    <%= f.file_field :photo %>
    </div>

    <%= f.submit %>
<% end %>